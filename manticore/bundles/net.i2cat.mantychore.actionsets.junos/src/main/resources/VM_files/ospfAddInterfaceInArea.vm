<configuration>
	#set($logicalRouter = "false" )
	#if(!$elementName.equals("") )
		#set( $logicalRouter = "true" )
	#end
	#if($logicalRouter.equals("true"))
		<logical-systems>
			<name>$elementName</name>
	#end
	<protocols>	
		<ospf>
		#set($ospfArea = $param.getOSPFArea())
			<area>
				<name>$ipUtilsHelper.ipv4LongToString($param.getAreaID())</name>
				#foreach($ospfPep in $param.getEndpointsInArea())
				<interface>
					<name>$ospfPep.getName()</name>
					##ENABLED/DISABLED
					#if($ospfPep.getEnabledState().toString().equals($disabledState))
					<disable/>
					#else
					<disable operation="delete"/>
					#end
				</interface>
				#end
			</area>
		</ospf>
	</protocols>
	#if($logicalRouter.equals("true"))
		</logical-systems>
	#end
</configuration>