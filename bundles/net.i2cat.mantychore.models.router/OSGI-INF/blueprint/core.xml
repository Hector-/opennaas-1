<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">
<bean id="routerModelFactory" 
	class="net.i2cat.mantychore.models.router.RouterModelFactory"/>
	<service ref="routerModelFactory"
		interface="com.iaasframework.capabilities.model.IResourceModelFactory" >
		<service-properties>
				<!--  identify the model router. It is generic for any router -->
			<entry key="model.name" value="router" />
			<entry key="model.version" value="1.0.0" />
		</service-properties>
	</service>
</blueprint>