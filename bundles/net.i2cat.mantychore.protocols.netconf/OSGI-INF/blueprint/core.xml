<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">
<bean id="netconfProtocolSessionFactory" 
	class="net.i2cat.mantychore.protocols.netconf.NetconfProtocolSessionFactory"/>
	<service ref="netconfProtocolSessionFactory"
		interface="com.iaasframework.capabilities.protocol.IProtocolSessionFactory" >
		<service-properties>
				<!--  identify the model router. It is generic for any router -->
			<entry key="protocol" value="Netconf" />
			<entry key="protocol.version" value="1.0.0" />
		</service-properties>
	</service>
</blueprint>